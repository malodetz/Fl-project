{
    "name": "l-language-highlight",
    "displayName": "L-language highlight",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.61.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "l-lang",
            "aliases": ["L-language", "l-lang"],
            "extensions": [".lang"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "l-lang",
            "scopeName": "source.lang",
            "path": "./syntaxes/l-lang.tmLanguage.json"
        }]
    }
}